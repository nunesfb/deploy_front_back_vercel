{
  "version": 2,
  "functions": {
    "backend/index.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/backend/(.*)", "dest": "/backend/index.ts" },
    { "src": "^/$", "status": 302, "dest": "/frontend/" },
    { "handle": "filesystem" },
    { "src": "/frontend/(.*)", "dest": "/$1" },
    { "src": "/frontend/?(.*)", "dest": "/index.html" }
  ]
}
